{
  "version": 8,
  "name": "air_quality",
  "center": [0, 0],
  "zoom": 3,
  "glyphs": "{fontstack}/{range}",
  "sources": {
    "air_quality": {
      "type": "geojson",
      "data": "https://eox-gtif-a.s3.eu-central-1.amazonaws.com/GTIF/new_data/geojson/{{time}}.geojson"
    }
  },
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "rgba(0,0,0,0)"
      }
    },
    {
      "id": "minHR",
      "type": "fill",
      "source": "air_quality",
      "filter": ["all"],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": [
          "interpolate",
          ["linear"],
          ["get", "Min Increased health risk"],
          2.8,
          ["to-color", "#00531e"],
          2.9,
          ["to-color", "#c3e556"],
          3.0,
          ["to-color", "#ffdd56"],
          3.1,
          ["to-color", "#f67758"],
          3.2,
          ["to-color", "#ff973f"],
          3.3,
          ["to-color", "#ff6331"],
          3.4,
          ["to-color", "#d5001f"],
          3.5,
          ["to-color", "#63000d"]
        ]
      }
    },
    {
      "id": "maxHR",
      "type": "fill",
      "source": "air_quality",
      "filter": ["all"],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": [
          "interpolate",
          ["linear"],
          ["get", "Max Increased health risk"],
          2.8,
          ["to-color", "#00531e"],
          2.9,
          ["to-color", "#c3e556"],
          3.0,
          ["to-color", "#ffdd56"],
          3.1,
          ["to-color", "#f67758"],
          3.2,
          ["to-color", "#ff973f"],
          3.3,
          ["to-color", "#ff6331"],
          3.4,
          ["to-color", "#d5001f"],
          3.5,
          ["to-color", "#63000d"]
        ]
      }
    },
    {
      "id": "meanHR",
      "type": "fill",
      "source": "air_quality",
      "filter": ["all"],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": [
          "interpolate",
          ["linear"],
          ["get", "Mean Increased health risk"],
          2.8,
          ["to-color", "#00531e"],
          2.9,
          ["to-color", "#c3e556"],
          3.0,
          ["to-color", "#ffdd56"],
          3.1,
          ["to-color", "#f67758"],
          3.2,
          ["to-color", "#ff973f"],
          3.3,
          ["to-color", "#ff6331"],
          3.4,
          ["to-color", "#d5001f"],
          3.5,
          ["to-color", "#63000d"]
        ]
      }
    }
  ]
}